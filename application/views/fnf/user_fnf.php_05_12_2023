<script>
  $(document).ready(function() {
    $('#show_datatable').DataTable();
    $('#mwp_ids').on('keypress', function(event) {
      var regex = new RegExp("^[a-zA-Z0-9]+$");
      var key = String.fromCharCode(!event.charCode ? event.which : event.charCode);
      if (!regex.test(key)) {
        event.preventDefault();
        return false;
      }
    });
  });
</script>
<script>
  $(function() {
    $('#multiselect').multiselect();
    $('#foffice_id').multiselect({
      includeSelectAllOption: true,
      enableFiltering: true,
      enableCaseInsensitiveFiltering: true,
      filterPlaceholder: 'Search for something...'
    });
  });
  get_list = (val) => {
    var cn = 1;
    $(".no_data").html("");
    $('.table_data_val').html("");
    $(".fnf_type").val(val); //setter 
    URL = '<?php echo base_url('fnf/fetch_data_according_category') ?>';
    let foffice_id = $(".foffice_id").val();
    let form_date = $(".form_date").val();
    let to_date = $(".to_date").val();
    let mwp_ids = $(".mwp_ids").val();
    let status = $("#status_fnf_serach").val();
    $("#page_open").val(val);
    $.ajax({
      url: URL,
      type: "GET",
      data: {
        office_id: foffice_id,
        form_date: form_date,
        to_date: to_date,
        mwp_ids: mwp_ids,
        val: val,
        status: status
      },
      beforeSend: function() {
        $(".loading-image").show();
      },
      dataType: "text",
      success: function(json_obj) {
        $(".loading-image").hide();
        var alrt = JSON.parse(json_obj);
        console.log(alrt.hr_checklist);
        $(".onload_tr_value").hide();
        if (val == 'hr_fnf') {
          var URL_new = '<?php echo base_url('fnf/fnf_export_report_excel') ?>';
          if (alrt.philippines_fnf == true) {
            $('.a_id').html("<a class='btn btn_padding filter_btn btn-sm' href=" + URL_new + "?office=" + foffice_id + "&f_date=" +
              form_date + "&t_date=" + to_date + "&type=3&excel=1&access=philippines_fnf>Export to Excel</a>");
          } else {
            $('.a_id').html("<a class='btn btn_padding filter_btn btn-sm' href=" + URL_new + "?office=" + foffice_id + "&f_date=" +
              form_date + "&t_date=" + to_date + "&type=7&excel=1>Export to Excel</a>");
          }
        }
        if (val == 'local_it') {
          var URL_new = '<?php echo base_url('fnf/fnf_export_report_excel') ?>';
          $('.a_id').html("<a class='btn btn_padding filter_btn btn-sm' href=" + URL_new + "?office=" + foffice_id + "&f_date=" +
            form_date + "&t_date=" + to_date + "&type=1&excel=1>Export to Excel</a>");
        }
        if (val == 'it_network') {
          var URL_new = '<?php echo base_url('fnf/fnf_export_report_excel') ?>';
          $('.a_id').html("<a class='btn btn_padding filter_btn btn-sm' href=" + URL_new + "?office=" + foffice_id + "&f_date=" +
            form_date + "&t_date=" + to_date + "&type=2&excel=1>Export to Excel</a>");
        }
        if (val == 'it_global') {
          var URL_new = '<?php echo base_url('fnf/fnf_export_report_excel') ?>';
          $('.a_id').html("<a class='btn btn_padding filter_btn btn-sm' href=" + URL_new + "?office=" + foffice_id + "&f_date=" +
            form_date + "&t_date=" + to_date + "&type=3&excel=1>Export to Excel</a>");
        }
        if (val == 'procurement') {
          var URL_new = '<?php echo base_url('fnf/fnf_asset_export_report_excel') ?>';
          $('.a_id').html("<a class='btn btn_padding filter_btn btn-sm' href=" + URL_new + "?office=" + foffice_id + "&f_date=" +
            form_date + "&t_date=" + to_date + "&type=1&excel=1>Export to Excel</a>");
        }
        if (val == 'hr_acceptance') {
          var URL_new = '<?php echo base_url('fnf/fnf_asset_export_report_excel') ?>';
          $('.a_id').html("<a class='btn btn_padding filter_btn btn-sm' href=" + URL_new + "?office=" + foffice_id + "&f_date=" +
            form_date + "&t_date=" + to_date + "&type=2&excel=1>Export to Excel</a>");
        }
        if (val == 'account_access_philip') {
          var URL_new = '<?php echo base_url('fnf/fnf_export_report_excel') ?>';
