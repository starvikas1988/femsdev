            $('#hr_checklist_tab').attr({
              style: "display: none"
            });
            $('#philippines_hr_checklist_tab').attr({
              style: "display: block"
            });
          } else {
            $('#hr_checklist_tab').attr({
              style: "display: block"
            });
            $('#philippines_hr_checklist_tab').attr({
              style: "display: none"
            });
          }
        }
        var html = '';
        if (alrt.hr_checklist.length > 0) {
